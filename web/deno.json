{
	"name": "@deno/web",
	"version": "1.0.0",
	"exports": "./src/main.tsx",
	"tasks": {
		"dev": "deno run -RNESW --allow-ffi --allow-run --node-modules-dir npm:vite",
		"build": "deno run -RESW --allow-ffi --allow-run --node-modules-dir npm:vite build",
		"preview": "deno run -RNESW --allow-ffi --allow-run --node-modules-dir npm:vite preview",
		"serve": "deno run -RN jsr:@std/http@1/file-server dist/"
	},
	"imports": {
		"@deno/vite-plugin": "npm:@deno/vite-plugin@1.0.0",
		"@types/react": "npm:@types/react@18.3.11",
		"@types/react-dom": "npm:@types/react-dom@18.3.1",
		"@vitejs/plugin-react": "npm:@vitejs/plugin-react@4.3.3",
		"hono": "jsr:@hono/hono@4.6.11",
		"react": "npm:react@18.3.1",
		"react-dom": "npm:react-dom@18.3.1",
		"vite": "npm:vite@5.4.9"
	}
}
