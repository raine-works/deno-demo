{
	"name": "@deno/demo",
	"version": "1.0.0",
	"exports": "./src/main.ts",
	"tasks": {
		"build": "deno compile --allow-net -o .build/app src/main.ts",
		"dev": "deno run --allow-net --watch src/main.ts",
		"load": "k6 run --vus 1000 --duration 60s k6/load.ts"
	},
	"imports": {
		"hono": "jsr:@hono/hono@4.6.7",
		"expect": "jsr:@std/expect@1.0.6",
		"uuid": "jsr:@std/uuid@1.0.4",
		"k6/http": "npm:k6/http"
	}
}
